{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "graphql": true,
    "window": true,
    "hljs": true
  },
  "rules": {
    "semi": "off",
    "comma-dangle": "off",
    "react/no-danger": "off",
    "react/prop-types": "off",
    "class-methods-use-this": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "react/jsx-filename-extension": "off",
    "import/no-extraneous-dependencies": "off"
  }
}