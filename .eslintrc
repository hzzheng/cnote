{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "graphql": true
  },
  "rules": {
    "semi": "off",
    "comma-dangle": "off",
    "react/prop-types": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "react/jsx-filename-extension": "off",
    "import/no-extraneous-dependencies": "off"
  }
}